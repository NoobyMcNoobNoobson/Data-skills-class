def main():
    list_of_numbers = [23,22.7,4,5,67.3,34,3,4,1,1,2]
    odd_total = 0
    even_toal = 0
    for list in list_of_numbers:
        if int(list)%2 == 0:
            even_toal += list
        elif int(list)%2 !=0:
            odd_total += list
    print('The total of all the odd number is ', odd_total)
    print('The total of all the even number is ', even_toal)

main()
